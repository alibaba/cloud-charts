{
  "autoSort": {
    "name": "数据自动排序",
    "type": "switch",
    "default": false,
    "description": "开启后将根据每一项的数值作降序排序"
  },
  "cycle": {
    "name": "是否为环图",
    "type": "switch",
    "default": false,
    "description": ""
  },
  "innerRadius": {
    "name": "内环半径比例",
    "type": "stepper",
    "step": 0.05,
    "min": 0,
    "max": 1,
    "default": 0.8,
    "description": "",
    "showInPanel": {
      "conditions": [
        [
          ".cycle",
          "$eq",
          true
        ]
      ],
      "logicalType": "$or"
    }
  },
  "select": {
    "name": "是否允许鼠标选中",
    "type": "switch",
    "default": false,
    "description": ""
  },
  "label": {
    "name": "显示标签",
    "type": "switch",
    "default": false,
    "description": ""
  },
  "outerRadius": {
    "name": "外环半径比例",
    "type": "stepper",
    "step": 0.05,
    "min": 0,
    "max": 1,
    "default": 0.8,
    "description": ""
  },
  "legend": {
    "name": "图例项",
    "type": "group",
    "fold": false,
    "children": {
      "visible": {
        "name": "显示图例",
        "type": "switch",
        "default": true,
        "description": ""
      },
      "showData": {
        "name": "显示数据",
        "type": "switch",
        "default": true,
        "description": ""
      }
    }
  },
  "tooltip": {
    "name": "提示信息",
    "type": "group",
    "fold": false,
    "children": {
      "visible": {
        "name": "显示提示信息",
        "type": "switch",
        "default": true,
        "description": ""
      },
      "showColon": {
        "name": "显示冒号",
        "type": "switch",
        "default": true,
        "description": ""
      }
    }
  }
}
